    <!-- 获取医疗收费代码视图 -->
    <select id="getAll" resultMap="rm">
        <![CDATA[
        SELECT *
          FROM 
             (
               SELECT row_number() over(ORDER BY fee_no) AS rownumber,* FROM his_fee_info
